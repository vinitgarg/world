pipeline{
           agent any
           tools{
                   maven 'Maven'
                  }
             stages {
                      stage("Build Stage"){
                         steps{
                                 bat 'mvn package'
                                 
                                 }
                        }
                       stage('Compile Stage')
                       {
                           steps{
                                   bat 'mvn clean compile'
                                 }
                        }         
                                 
                         stage('Testing Stage')
                         {
                             steps{
                                     bat 'mvn test'
                                  }
                            }
                     }       
                            
                            
        }
                                